{
    "//": "NOTE: custom asymmetric changes in italian translation. See the 'bw=Black and White|colors=Colors' translation above.",
    "strings": {
        "//": "NOTE: this is legacy code. We used to have custom keys in the italian template used in VOXmail, so we have to keep using them. See also the custom replacement rules",
        "bw=Bianco e Nero|colors=Colori": "Bianco e Nero=Bianco e Nero|Colori=Colori"
    },
    "partial": true,
    "customReplacements": {
        "style": [
            {
                "replace": "socialIconType=Colori ",
                "search": "socialIconType=colors "
            }
        ],
        "custom-row[if]": [
            {
                "attribute": "if",
                "replace": "socialIconType eq 'Bianco e Nero'",
                "search": "socialIconType eq 'bw'"
            },
            {
                "attribute": "if",
                "replace": "socialIconType eq 'Colori'",
                "search": "socialIconType eq 'colors'"
            }
        ]
    }
}